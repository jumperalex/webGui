	padding-left: 16px;
	padding-right: 0;
}
table.disk_status.right tr > td + td + td + td {
	text-align: right;
	padding-left: 0;
	padding-right: 16px;
}
table.disk_status.center tr > td + td + td + td {
	text-align: center;
	padding-left: 8px;
	padding-right: 8px;
}
table.disk_status select {
	background: none;
}
table.disk_status tr:hover td{
	background: #f0f0f0;
}
table.disk_status tr:first-child:hover td {
	background: #E0E0E0; /* old browsers */
