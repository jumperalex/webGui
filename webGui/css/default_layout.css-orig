}
input[type=text], input[type=password], textarea {
	background: #FFF;
	min-height: 20px;
	border: 1px solid #dcdcdc;
	padding: 2px 6px;
	line-height: 20px;
	outline: none;
	-webkit-box-shadow: inset 2px 2px 6px #eef0f0;
	-moz-box-shadow: inset 2px 2px 6px #eef0f0;
	box-shadow: inset 2px 2px 6px #eef0f0;
	width:305px;
	margin-top:0;
	margin-right: 10px;
}
input[type=submit], input[type=reset], input[type=button], button[type=button], a.button {
	position: relative;
	overflow: visible;
	display: inline-block;
	padding: 5px 10px;
	border: 1px solid #d4d4d4;
	margin: 7px 14px 0 0;
	text-decoration: none;
	text-shadow: 1px 1px 0 #ffffff;
	font:11px/normal sans-serif;
	color: #333333;
	white-space: nowrap;
	cursor: pointer;
	outline: none;
	background-color: #ececec;
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#f4f4f4), to(#ececec));
	background-image: -moz-linear-gradient(#f4f4f4, #ececec);
	background-image: -o-linear-gradient(#f4f4f4, #ececec);
	background-image: linear-gradient(#f4f4f4, #ececec);
	-webkit-background-clip: padding;
	-moz-background-clip: padding;
	-o-background-clip: padding-box;
	/*background-clip: padding-box;*/ /* commented out due to Opera 11.10 bug */
	-webkit-border-radius: 0.2em;
	-moz-border-radius: 0.2em;
	border-radius: 0.2em;
	/* IE hacks */
	zoom: 1;
	*display: inline;
}
input:focus[type=text], input:focus[type=password], textarea[type=text] {
	background: #F7F7F7;
}
input:hover[type=submit], input:hover[type=reset], input:hover[type=button], button:hover[type=button], a.button:hover {
	border-color: #76b347;
	border-bottom-color: #2a65a0;
	text-decoration: none;
	text-shadow: -1px -1px 0 rgba(0,0,0,0.3);
	color: #ffffff;
	background-color: #6FA239;
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#76b347), to(#5e9e2e));
	background-image: -moz-linear-gradient(#76b347, #5e9e2e);
	background-image: -o-linear-gradient(#76b347, #5e9e2e);
	background-image: linear-gradient(#76b347, #5e9e2e);
}
input:active[type=submit], input:active[type=reset], input:active[type=button], button:active[type=button] {
	border-color: #2a65a0;
	border-bottom-color: #3884cf;
	-moz-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eee;
	-webkit-box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eee;
	box-shadow: inset 0 0 8px 4px #548c29, 0 1px 0 0 #eee;
}
input[disabled], input:hover[type=reset][disabled], input:hover[type=button][disabled], input:hover[type=submit][disabled], button[disabled], button:hover[disabled], input:active[disabled], input:active[type=submit][disabled], input:active[type=reset][disabled], input:active[type=button][disabled], button:hover[type=button][disabled], button[type=button][disabled], button:active[type=button][disabled], button:active[disabled], button:active[disabled], textarea[disabled] {
	color: #CCCCCC;
	text-shadow: 1px 1px 0 #ffffff;
	border: 1px solid #ececec;
	background-color: #F9F9F9;
	background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#F9F9F9), to(#F3F3F3));
	background-image: -moz-linear-gradient(#F9F9F9, #F3F3F3);
	background-image: -o-linear-gradient(#F9F9F9, #F3F3F3);
	background-image: linear-gradient(#F9F9F9, #F3F3F3);
	-moz-box-shadow: none;
	-webkit-box-shadow: none;
	box-shadow: none;
	cursor: default;
}
select {
	padding: 4px;
	margin: 0;
	border-radius:4px;
	-moz-box-shadow: 0 2px 0 #dcdcdc,0 -1px #fff inset;
	-webkit-box-shadow: 0 2px 0 #dcdcdc, 0 -1px #fff inset;
	box-shadow: 0 2px 0 #dcdcdc, 0 -1px #fff inset;
	background: #F9F9F9;
	color:#333333;
	border:none;
	outline:none;
	display: inline-block;
	cursor:pointer;
	margin-right: 10px;
}
select[disabled] {
	background-color: #f9f9f9;
	text-shadow: 1px 1px 0 #ffffff;
	color: #cccccc;
	cursor:default;
	-moz-box-shadow: 0 2px 0 #ececec, 0 -1px #fff inset;
	-webkit-box-shadow: 0 2px 0 #ececec, 0 -1px #fff inset;
	box-shadow: 0 2px 0 #ececec, 0 -1px #fff inset;
}
.Confirm {
	white-space: nowrap;
}
.Confirm input[type=checkbox] {
	vertical-align: bottom;
}
