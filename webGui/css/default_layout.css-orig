html, body, iframe {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #222222;
	background-color: #FFFFFF;
	padding: 0;
	margin: 0;
}
body {
	margin-bottom: 150px;
}
#template {
	min-width: 960px;
/*	max-width: 1200px; */
	margin: 0 auto;
}
img {
	border: 0;
