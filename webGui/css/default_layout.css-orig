table.disk_status {
	border-collapse: collapse;
}
table.disk_status td span {
	margin-left: 10px;
}
table.disk_status tr:first-child {
	font-weight:bold;
	color: #333333;
	background: #E0E0E0; /* old browsers */
	background: -moz-linear-gradient(top, #E9E9E9 0%, #E0E0E0 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#E9E9E9), color-stop(100%,#E0E0E0)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#E9E9E9', endColorstr='#E0E0E0',GradientType=0 ); /* ie */
