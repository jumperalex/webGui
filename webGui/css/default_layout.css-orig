}
table.disk_status tr > td {
	padding-left: 12px;
	padding-right: 0;
}
table.disk_status tr > td + td + td {
	text-align: center;
	padding-left: 4px;
	padding-right: 8px;
}
table.disk_status.stats tr > td + td {
	text-align: left;
	padding-left: 0;
	padding-right: 12px;
}
