}
#nav_block #nav_item:hover {
	background: #3b3f40; /* old browsers */
	background: -moz-linear-gradient(top, #3b3f40 0%, #28292d 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#3b3f40), color-stop(100%,#28292d)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#3b3f40', endColorstr='#28292d',GradientType=0 ); /* ie */
	border-bottom: solid thick #6fa239;
	border-radius: 5px;
}
#nav_block #nav_item.active {
	border-right: solid thin #555555;
	border-bottom: solid thick #6fa239;
	background: #000; /* old browsers */
	background: -moz-linear-gradient(top, #333333 0%, #000000 100%); /* firefox */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#333333), color-stop(100%,#000000)); /* webkit */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#333333', endColorstr='#000000',GradientType=0 ); /* ie */
	color: #fff;
	border-radius: 5px;
}
#nav_block #nav_item a {
	color: #f0f0f0;
	font-style: normal;
	display: block;
	padding-left: 10px;
	padding-right: 10px;
	min-width: 48px;
}
#nav_block #nav_item a:hover {
	color: #9cd670;
}
#nav_block #nav_item.active a {
	color: #ffffff;
}
#nav_block #nav_item.active:hover a {
